<h1>New User</h1>

<%= render "/layouts/errors" %>

<%= form_for @user  do |f| %>
  <p>
  <%= f.label :first_name %>
  <%= f.text_field :first_name%>
  </p>
  <p>
  <%= f.label :username %>
  <%= f.text_field :username%>
  </p>
  <p>
  <%= f.label :profile_pic %>
  <%= f.text_field :profile_pic %>
  </p>
  <p>
  <%= f.label :age %>
  <%= f.number_field :age%>
  </p>
  <p>
  <%= f.label :city %>
  <%= f.text_field :city%>
  </p>
  <p>
  <%= f.label :bio %>
  <%= f.text_area :bio%>
  </p>
  <p>
  <%= f.submit %>
  </p>
<% end %>
